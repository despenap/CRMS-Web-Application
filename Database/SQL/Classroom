CREATE TABLE `classroom` (
  `classroom_id` int(11) NOT NULL,
  `classroom_name` varchar(255) NOT NULL,
  `classroom_building` varchar(255) DEFAULT NULL,
  `classroom_address` int(11) DEFAULT NULL,
  `classroom_capacity` int(11) DEFAULT NULL,
  `classroom_is_publicly_available` tinyint(1) DEFAULT NULL,
  `classroom_has_projector` tinyint(1) DEFAULT NULL,
  `classroom_type` int(11) DEFAULT NULL,
  `classroom_available_hours` int(11) DEFAULT NULL,
  `classroom_available_days` int(11) DEFAULT NULL
)
